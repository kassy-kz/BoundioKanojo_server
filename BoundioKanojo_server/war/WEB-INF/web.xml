<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
    <servlet>
        <servlet-name>TelStore</servlet-name>
        <servlet-class>orz.kassy.boundiokanojo.server.TelStoreServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TelStore</servlet-name>
        <url-pattern>/telstore</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>TelReq</servlet-name>
        <servlet-class>orz.kassy.boundiokanojo.server.TelReqServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TelReq</servlet-name>
        <url-pattern>/telreq</url-pattern>
    </servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
